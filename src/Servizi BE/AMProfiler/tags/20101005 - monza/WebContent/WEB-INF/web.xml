 <display-name>AMProfiler1</display-name>
 <filter>
  <filter-name>Filtro</filter-name>
  <filter-class>it.webred.permessi.Filtro</filter-class>
  <init-param>
   <param-name>driverClass</param-name>
   <param-value>oracle.jdbc.driver.OracleDriver</param-value>
  </init-param>
  <init-param>
   <param-name>urlConn</param-name>
   <param-value>jdbc:oracle:thin:@10.10.33.25:1521:ORCL</param-value>
  </init-param>
  <init-param>
   <param-name>userConn</param-name>
   <param-value>AM</param-value>
  </init-param>
  <init-param>
   <param-name>pwdConn</param-name>
   <param-value>AM</param-value>
  </init-param>
  <init-param>
   <param-name>numGiorniVal</param-name>
   <param-value>90</param-value>
  </init-param>
  <init-param>
   <param-name>paginaCambioPwd</param-name>
   <param-value>http://localhost:8080/AMProfiler/SalvaUtente?pwdScaduta=true&amp;mode=vis</param-value>
  </init-param>
 </filter>
 <!-- 
	<filter-mapping>
