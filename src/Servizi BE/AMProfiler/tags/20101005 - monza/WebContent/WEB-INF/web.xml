  <welcome-file>default.htm</welcome-file>
  <welcome-file>default.jsp</welcome-file>
 </welcome-file-list>
 <security-constraint>
  <!-- Sample Security Constraint -->
  <web-resource-collection>
   <web-resource-name>protected-resources</web-resource-name>
   <url-pattern>/CaricaMenu</url-pattern>
   <url-pattern>/CancellaUtente</url-pattern>
   <url-pattern>/CaricaPermessi</url-pattern>
   <url-pattern>/CaricaServizi</url-pattern>
   <url-pattern>/MenuBulder</url-pattern>
   <url-pattern>/NuovoGruppo</url-pattern>
   <url-pattern>/NuovoPermesso</url-pattern>
   <url-pattern>/SalvaPermessi</url-pattern>
   <url-pattern>/SalvaServiziRuoli</url-pattern>
   <url-pattern>/SalvaRuoliGruppo</url-pattern>
   <url-pattern>/SalvaRuoliUtente</url-pattern>
   <url-pattern>/SalvaUtente</url-pattern>
   <url-pattern>*.jsf</url-pattern>
   <url-pattern>*.jsp</url-pattern>
   <url-pattern>*.html</url-pattern>
   <url-pattern>*.htm</url-pattern>
   <http-method>HEAD</http-method>
   <http-method>GET</http-method>
   <http-method>POST</http-method>
   <http-method>PUT</http-method>
   <http-method>DELETE</http-method>
  </web-resource-collection>
  <auth-constraint>
   <role-name>AMProfiler</role-name>
  </auth-constraint>
  <user-data-constraint>
   <transport-guarantee>NONE</transport-guarantee>
  </user-data-constraint>
 </security-constraint>
 <login-config>
  <auth-method>FORM</auth-method>
	<form-login-config>
	<form-login-page>/jsp/login.jsp</form-login-page>
	<form-error-page>/jsp/loginfail.jsp</form-error-page>
	</form-login-config>   
 </login-config>
 <security-role>
  <description>Ruolo di accesso applicativo</description>
  <role-name>AMProfiler</role-name>
 </security-role>
</web-app>
