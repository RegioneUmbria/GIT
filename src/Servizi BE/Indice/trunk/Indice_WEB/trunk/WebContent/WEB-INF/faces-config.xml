<?xml version="1.0"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>userBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.common.UserBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>indiceCommonBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.common.IndiceCommonBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>uiPageBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.common.UIPageBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>dataProviderImpl</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.common.pagination.DataProviderImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>unicoDataModel</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.common.pagination.UnicoDataModel</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>dataProvider</property-name>
   <property-class>it.webred.ct.service.indice.web.common.pagination.DataProvider</property-class>
   <value>#{dataProviderImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>totaleDataModel</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.common.pagination.TotaleDataModel</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>dataProvider</property-name>
   <property-class>it.webred.ct.service.indice.web.common.pagination.DataProvider</property-class>
   <value>#{dataProviderImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>suggestionViaBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.via.SuggestionViaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>viaBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.via.ViaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>suggestionSoggettoBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.soggetto.SuggestionSoggettoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>soggettoBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.soggetto.SoggettoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>oggettoBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.oggetto.OggettoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>civicoDataProviderImpl</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.civico.CivicoDataProviderImpl</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>civicoDataModel</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.indice.web.civico.CivicoDataModel</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>dataProvider</property-name>
   <property-class>it.webred.ct.service.indice.web.civico.CivicoDataProvider</property-class>
   <value>#{civicoDataProviderImpl}</value>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <navigation-case>
   <from-outcome>ricerca.soggetto</from-outcome>
   <to-view-id>/jsp/protected/template/templateSoggetto.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>ricerca.oggetto</from-outcome>
   <to-view-id>/jsp/protected/template/templateOggetto.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>ricerca.via</from-outcome>
   <to-view-id>/jsp/protected/template/templateVia.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
 </application>
</faces-config>
