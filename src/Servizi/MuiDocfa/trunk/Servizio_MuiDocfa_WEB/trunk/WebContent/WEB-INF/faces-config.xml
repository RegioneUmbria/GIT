 </managed-bean>
 <managed-bean>
  <managed-bean-name>suggestionBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.util.SuggestionBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>exportPDFBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.export.ExportPDFBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>exportXLSBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.export.ExportXLSBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>exportTXTBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.export.ExportTXTBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>iciSubjectBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.ici.SubjectBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>iciDetailBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.ici.DetailBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>iciDataProviderImpl</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.ici.pagination.DataProviderImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>iciDataModel</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.ici.pagination.DataModel</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>dataProvider</property-name>
   <property-class>it.webred.ct.service.muidocfa.web.bean.ici.pagination.DataProvider</property-class>
   <value>#{iciDataProviderImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tarSubjectBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.tarsu.SubjectBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tarDetailBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.tarsu.DetailBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>planimetrieC340Bean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.tarsu.planimetrie.PlanimetrieC340Bean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>planimetrieDocfaBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.tarsu.planimetrie.PlanimetrieDocfaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tarDataProviderImpl</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.tarsu.pagination.DataProviderImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tarDataModel</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.tarsu.pagination.DataModel</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>dataProvider</property-name>
   <property-class>it.webred.ct.service.muidocfa.web.bean.tarsu.pagination.DataProvider</property-class>
   <value>#{tarDataProviderImpl}</value>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>muidocfa.overview</from-outcome>
   <to-view-id>/jsp/protected/home.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>muidocfa.search.ici</from-outcome>
   <to-view-id>/jsp/protected/search/ici/search.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>muidocfa.ici.detail</from-outcome>
   <to-view-id>/jsp/protected/search/ici/detail/detail.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>muidocfa.search.tarsu</from-outcome>
   <to-view-id>/jsp/protected/search/tarsu/search.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>muidocfa.tarsu.detail</from-outcome>
   <to-view-id>/jsp/protected/search/tarsu/detail/detail.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <message-bundle>it.webred.ct.service.muidocfa.web.resources.messages</message-bundle>
 </application>
</faces-config>
