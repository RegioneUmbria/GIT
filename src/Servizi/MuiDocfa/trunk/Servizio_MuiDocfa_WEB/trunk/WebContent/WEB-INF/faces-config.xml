  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.overview.OverviewBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pageUiBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.muidocfa.web.bean.util.PageUiBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
