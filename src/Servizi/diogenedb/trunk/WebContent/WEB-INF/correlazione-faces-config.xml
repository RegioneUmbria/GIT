<?xml version="1.0"?>
<!DOCTYPE faces-config PUBLIC "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.1//EN" "http://java.sun.com/dtd/web-facesconfig_1_1.dtd">
<faces-config>

	<managed-bean>
	    <managed-bean-name>MenuCorrelazione</managed-bean-name>
	    <managed-bean-class>it.webred.diogene.correlazione.backingbeans.MenuCorrelazioneBB</managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
	    <managed-bean-name>Temi</managed-bean-name>
	    <managed-bean-class>it.webred.diogene.correlazione.backingbeans.TemiBB</managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
	    <managed-bean-name>TipiEtichetta</managed-bean-name>
	    <managed-bean-class>it.webred.diogene.correlazione.backingbeans.TipiEtichettaBB</managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
	    <managed-bean-name>ListaTemi</managed-bean-name>
	    <managed-bean-class>it.webred.diogene.correlazione.backingbeans.ListaTemiBB</managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
	    <managed-bean-name>ListaTipiEtichetta</managed-bean-name>
	    <managed-bean-class>it.webred.diogene.correlazione.backingbeans.ListaTipiEtichettaBB</managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
	    <managed-bean-name>ListaArchivi</managed-bean-name>
	    <managed-bean-class>it.webred.diogene.correlazione.backingbeans.ListaArchiviBB</managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
	    <managed-bean-name>Ricerche</managed-bean-name>
	    <managed-bean-class>it.webred.diogene.correlazione.backingbeans.RicercheBB</managed-bean-class>
	    <managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>VisualizzaDati</managed-bean-name>
		<managed-bean-class>it.webred.diogene.correlazione.backingbeans.VisualizzaDatiBB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
		<managed-property>
			<property-name>idxRicerca</property-name>
			<value>#{param.idxRicerca}</value>
		</managed-property>
		<managed-property>
			<property-name>idxArchivio</property-name>
			<value>#{param.idxArchivio}</value>
		</managed-property>
		<managed-property>
			<property-name>pagina</property-name>
			<value>#{param.pagina}</value>
		</managed-property>
	</managed-bean>
	
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAMenuCorrelazione</from-outcome>
	    	<to-view-id>/correlazione/MenuCorrelazione.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAGestioneTemi</from-outcome>
	    	<to-view-id>/correlazione/Temi.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAGestioneTipiEtichetta</from-outcome>
	    	<to-view-id>/correlazione/TipiEtichetta.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAListaTemi</from-outcome>
	    	<to-view-id>/correlazione/ListaTemi.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAListaTipiEtichetta</from-outcome>
	    	<to-view-id>/correlazione/ListaTipiEtichetta.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAListaArchivi</from-outcome>
	    	<to-view-id>/correlazione/ListaArchivi.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAValorizzaChiave</from-outcome>
	    	<to-view-id>/correlazione/ValorizzaChiave.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	<navigation-rule>
	  	<navigation-case>
	    	<from-outcome>vaiAVisualizzaDati</from-outcome>
	    	<to-view-id>/correlazione/VisualizzaDati.jsp</to-view-id>
	  	</navigation-case>
	</navigation-rule>
	
</faces-config>
