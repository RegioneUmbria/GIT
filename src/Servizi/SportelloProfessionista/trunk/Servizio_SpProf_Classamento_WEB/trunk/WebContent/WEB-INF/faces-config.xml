<?xml version="1.0"?>
<faces-config version="1.2" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd">
 <managed-bean>
  <managed-bean-name>userBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.spclass.web.bean.util.UserBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>pageBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.spclass.web.bean.util.PageBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>commonDataBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.spclass.web.bean.util.CommonDataBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classamentoUnoBck</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.spclass.web.bean.ClassamentoUnoBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classamentoDueBck</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.spclass.web.bean.ClassamentoDueBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classamentoTreBck</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.spclass.web.bean.ClassamentoTreBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>classamento.home</from-outcome>
   <to-view-id>/jsp/protected/home.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>classamento.calcolo1</from-outcome>
   <to-view-id>/jsp/protected/calcolo/tipo1.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>classamento.calcolo2</from-outcome>
   <to-view-id>/jsp/protected/calcolo/tipo2.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>classamento.calcolo3</from-outcome>
   <to-view-id>/jsp/protected/calcolo/tipo2.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>classamento.calcolo4</from-outcome>
   <to-view-id>/jsp/protected/calcolo/tipo3.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <message-bundle>it.webred.ct.service.spclass.web.resources.messages</message-bundle>
 </application>
</faces-config>
