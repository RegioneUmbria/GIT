   <value>#{GPCreditoService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>operazioniDataProviderImpl</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.operazione.OperazioniDataProviderImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>operazioniService</property-name>
   <property-class>it.webred.ct.service.gestprep.data.access.GestOperazioniService</property-class>
   <value>#{GPOperazioniService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>operazioniListDataModel</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.operazione.OperazioniListDataModel</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>dataProvider</property-name>
   <property-class>it.webred.cet.service.gestprep.web.beans.operazione.OperazioniDataProvider</property-class>
   <value>#{operazioniDataProviderImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>operazioneBean</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.operazione.OperazioneBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>operazioneService</property-name>
   <property-class>it.webred.ct.service.gestprep.data.access.GestOperazioniService</property-class>
   <value>#{GPOperazioniService}</value>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>gestprep.anagList</from-outcome>
   <to-view-id>/jsp/protected/anag/anagList.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gestprep.anagCreate</from-outcome>
   <to-view-id>/jsp/protected/anag/anagCreate.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gestprep.visList</from-outcome>
   <to-view-id>/jsp/protected/visure/visureList.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>gestprep.operList</from-outcome>
   <to-view-id>/jsp/protected/operazioni/operazioniList.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <variable-resolver>org.springframework.web.jsf.SpringBeanVariableResolver</variable-resolver>
  <message-bundle>it.webred.cet.service.gestprep.web.resources.messages</message-bundle>
 </application>
</faces-config>
