  <managed-property>
   <property-name>anagService</property-name>
   <property-class>it.webred.ct.service.gestprep.data.access.GestAnagService</property-class>
   <value>#{GPAnagService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>anagDataProviderImpl</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.anag.AnagDataProviderImpl</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>anagService</property-name>
   <property-class>it.webred.ct.service.gestprep.data.access.GestAnagService</property-class>
   <value>#{GPAnagService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>visuraBean</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.visura.VisuraBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>visureService</property-name>
   <property-class>it.webred.ct.service.gestprep.data.access.GestVisureService</property-class>
   <value>#{GPVisuraService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>tariffaBean</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.tariffa.TariffaBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>tariffeService</property-name>
   <property-class>it.webred.ct.service.gestprep.data.access.GestTariffeService</property-class>
   <value>#{GPTariffaService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>depositoBean</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.deposito.DepositoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>depositoService</property-name>
   <property-class>it.webred.ct.service.gestprep.data.access.GestDepositoService</property-class>
   <value>#{GPDepositoService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>creditoBean</managed-bean-name>
  <managed-bean-class>it.webred.cet.service.gestprep.web.beans.credito.CreditoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>creditoService</property-name>
