<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="it.webred.mui.model.CodiciDup" table="CODICI_DUP">
        <id name="codice" type="string">
            <column name="CODI_DIRITTO" length="8" not-null="true" />
        </id>
        <property name="descrizione" type="string">
            <column name="DESCR_DIRITTO" length="60"  />
        </property>
    </class>
</hibernate-mapping>
