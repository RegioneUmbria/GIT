  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>page</property-name>
   <property-class>java.lang.String</property-class>
   <value>..\empty.xhtml</value>
  </managed-property>
  <managed-property>
   <property-name>pageResultList</property-name>
   <property-class>java.lang.String</property-class>
   <value>/jsp/protected/empty.xhtml</value>
