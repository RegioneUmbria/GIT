 <navigation-rule>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>c340.search</from-outcome>
   <to-view-id>/jsp/protected/visualizzatore/listRicercaCatasto.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/jsp/protected/visualizzatore/listRicercaCatasto.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>comma340DettaglioBean.apriDettaglio</from-outcome>
   <to-view-id>/jsp/protected/visualizzatore/dettaglioImmobile.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/jsp/protected/visualizzatore/dettaglioImmobile.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>gestionePraticheBean.apriGestionePratiche</from-outcome>
   <to-view-id>/jsp/protected/gestione/gestionePratiche.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <variable-resolver>org.springframework.web.jsf.SpringBeanVariableResolver</variable-resolver>
  <message-bundle>it.webred.ct.service.comma340.web.resources.messages</message-bundle>
 </application>
</faces-config>
