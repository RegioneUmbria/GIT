   <property-class>it.webred.ct.config.parameters.ParameterService</property-class>
   <value>#{ParameterService}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>gestionePraticheBean</managed-bean-name>
  <managed-bean-class>it.webred.ct.service.comma340.web.pratica.GestionePraticheBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
