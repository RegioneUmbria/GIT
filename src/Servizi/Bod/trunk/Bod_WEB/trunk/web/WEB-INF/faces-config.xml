  <managed-bean-name>ricercaFiltroBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.RicercaFiltroBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>causaleService</property-name>
   <value>#{causaleServiceImpl}</value>
  </managed-property>
  <managed-property>
   <property-name>dichiaranteService</property-name>
   <value>#{dichiaranteServiceImpl}</value>
  </managed-property>
  <managed-property>
   <property-name>destinazioneFunzionaleService</property-name>
   <value>#{destinazioneFunzionaleServiceImpl}</value>
  </managed-property>
  <managed-property>
   <property-name>uiuService</property-name>
   <value>#{uiuServiceImpl}</value>
  </managed-property>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
  <managed-property>
   <property-name>categoriaService</property-name>
   <value>#{categoriaServiceImpl}</value>
  </managed-property>
  <managed-property>
   <property-name>datiGeneraliService</property-name>
   <value>#{datiGeneraliServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>ricercaListaBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.RicercaListaBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>lstBod</property-name>
   <property-class>java.util.ArrayList</property-class>
   <value>#{ricercaFiltroBck.lstBod}</value>
  </managed-property>
  <managed-property>
   <property-name>uiuService</property-name>
   <value>#{uiuServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean id="ricercaDettaglioBck">
  <managed-bean-name>ricercaDettaglioBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.RicercaDettaglioBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>selBodBean</property-name>
   <property-class>it.bod.application.beans.BodBean</property-class>
   <value>#{ricercaListaBck.selBodBean}</value>
  </managed-property>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>esportaFiltroBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.EsportaFiltroBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>statisticaFiltroBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.StatisticaFiltroBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>statisticaListaBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.StatisticaListaBck</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
  <managed-property>
   <property-name>lstDiagnostiche</property-name>
   <property-class>java.util.ArrayList</property-class>
   <value>#{statisticaFiltroBck.lstDiagnostiche}</value>
  </managed-property>
  <managed-property>
   <property-name>lstCategorie</property-name>
   <property-class>java.util.ArrayList</property-class>
   <value>#{statisticaFiltroBck.lstCategorie}</value>
  </managed-property>
  <managed-property>
   <property-name>fornituraDal</property-name>
   <property-class>java.lang.String</property-class>
   <value>#{statisticaFiltroBck.fornituraDal}</value>
  </managed-property>
  <managed-property>
   <property-name>fornituraAl</property-name>
   <property-class>java.lang.String</property-class>
   <value>#{statisticaFiltroBck.fornituraAl}</value>
  </managed-property>
  <managed-property>
   <property-name>currentDiagno</property-name>
   <property-class>java.lang.String</property-class>
   <value>#{statisticaFiltroBck.currentDiagno}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classamentoUnoBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.ClassamentoUnoBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>cbxFoglio</property-name>
   <property-class>it.webred.rich.common.ComboBoxRch</property-class>
   <value>#{classamentoBck.cbxFoglio}</value>
  </managed-property>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classamentoDueBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.ClassamentoDueBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>cbxFoglio</property-name>
   <property-class>it.webred.rich.common.ComboBoxRch</property-class>
   <value>#{classamentoBck.cbxFoglio}</value>
  </managed-property>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classamentoTreBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.ClassamentoTreBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <managed-property>
   <property-name>cbxFoglio</property-name>
   <property-class>it.webred.rich.common.ComboBoxRch</property-class>
   <value>#{classamentoBck.cbxFoglio}</value>
  </managed-property>
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>classamentoBck</managed-bean-name>
  <managed-bean-class>it.bod.application.backing.ClassamentoBck</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
  <!--<managed-property>
   <property-name>docfaService</property-name>
   <value>#{docfaService}</value>
  </managed-property>
  -->
  <managed-property>
   <property-name>bodLogicService</property-name>
   <value>#{bodLogicServiceImpl}</value>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <from-view-id>*</from-view-id>
  <!-- Inizio metodi della classe classamentoUnoBck -->
  <navigation-case>
   <from-outcome>classamentoUnoBck.init</from-outcome>
   <to-view-id>/pages/classamento/classamentoUno.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe classamentoUnoBck 
 Inizio metodi della classe classamentoDueBck -->
  <navigation-case>
   <from-outcome>classamentoDueBck.init</from-outcome>
   <to-view-id>/pages/classamento/classamentoDue.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe classamentoDueBck 
 Inizio metodi della classe classamentoTreBck -->
  <navigation-case>
   <from-outcome>classamentoTreBck.init</from-outcome>
   <to-view-id>/pages/classamento/classamentoTre.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe classamentoTreBck 
 Inizio metodi della classe ricercaDettaglioBck -->
  <navigation-case>
   <from-outcome>ricercaDettaglioBck.openPDF</from-outcome>
   <to-view-id>/pages/ricerca/showPdf.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ricercaDettaglioBck.getBodBeanSuccessivo</from-outcome>
   <to-view-id>/pages/ricerca/dettaglioRicerca.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ricercaDettaglioBck.getBodBeanPrecedente</from-outcome>
   <to-view-id>/pages/ricerca/dettaglioRicerca.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ricercaDettaglioBck.selectionChanged</from-outcome>
   <to-view-id>/pages/ricerca/dettaglioRicerca.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe ricercaDettaglioBck 
 Inizio metodi della classe menuBck -->
  <navigation-case>
   <from-outcome>menuBck.starting</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>menuBck.logout</from-outcome>
   <to-view-id>/logout-redirect.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe menuBck 
 Inizio metodi della classe classamentoBck -->
  <navigation-case>
   <from-outcome>classamentoBck.init</from-outcome>
   <to-view-id>/pages/classamento/menuClassamento.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe classamentoBck 
 Inizio metodi della classe statisticaFiltroBck -->
  <navigation-case>
   <from-outcome>statisticaFiltroBck.preparaFiltro</from-outcome>
   <to-view-id>/pages/statistica/homeStatistica.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>statisticaFiltroBck.ricerca</from-outcome>
   <to-view-id>/pages/statistica/listaStatistica.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe statisticaFiltroBck 
 Inizio metodi della classe ricercaFiltroBck -->
  <navigation-case>
   <from-outcome>ricercaFiltroBck.ricerca</from-outcome>
   <to-view-id>/pages/ricerca/listaRicerca.xhtml</to-view-id>
   <redirect/>
   <!-- Questo tag per far funzionare le regole di navigazione con IE7 -->
  </navigation-case>
  <navigation-case>
   <from-outcome>ricercaFiltroBck.preparaFiltro</from-outcome>
   <to-view-id>/pages/ricerca/homeRicerca.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ricercaFiltroBck.autocomplete</from-outcome>
   <to-view-id>/pages/ricerca/homeRicerca.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>ricercaFiltroBck.pulisci</from-outcome>
   <to-view-id>/pages/ricerca/homeRicerca.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe ricercaFiltroBck 
 Inizio metodi della classe esportaFiltroBck -->
  <navigation-case>
   <from-outcome>esportaFiltroBck.importaEsitiShow</from-outcome>
   <to-view-id>/pages/esporta/homeImporta.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>esportaFiltroBck.ricerca</from-outcome>
   <to-view-id>/pages/esporta/homeEsporta.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>esportaFiltroBck.confrontoShow</from-outcome>
   <to-view-id>/pages/esporta/homeConfronto.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <!-- Fine metodi della classe esportaFiltroBck -->
  <navigation-case>
   <from-outcome>failure</from-outcome>
   <to-view-id>/pages/nonAutorizzato.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 
 
 <validator>
  <validator-id>htmlValidator</validator-id>
  <validator-class>it.doro.tools.HtmlValidator</validator-class>
 </validator>
 <application>
  <view-handler>com.sun.facelets.FaceletViewHandler</view-handler>
  <variable-resolver>org.springframework.web.jsf.DelegatingVariableResolver</variable-resolver>
  <message-bundle>resources</message-bundle>
  <locale-config>
   <default-locale>it</default-locale>
  </locale-config>
 </application>
</faces-config>
