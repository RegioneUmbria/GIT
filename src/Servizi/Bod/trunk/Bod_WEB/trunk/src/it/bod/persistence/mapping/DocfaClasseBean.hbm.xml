<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="it.bod.application.beans.DocfaClasseBean" table="DOCFA_CLASSE">
  
	<composite-id mapped="false" unsaved-value="undefined">
	   <key-property column="ZONA" name="zona" type="java.lang.String"/>
	   <key-property column="CATEGORIA" name="categoria" type="java.lang.String"/>
	   <key-property column="CLASSE" name="classe" type="java.lang.String"/>
    </composite-id>

  <property column="TARIFFA" generated="never" lazy="false"   name="tariffa" type="java.math.BigDecimal"/>
   <property column="TARIFFA_EURO" generated="never" lazy="false"   name="tariffaEuro" type="java.math.BigDecimal"/>
  
 </class>
</hibernate-mapping>
