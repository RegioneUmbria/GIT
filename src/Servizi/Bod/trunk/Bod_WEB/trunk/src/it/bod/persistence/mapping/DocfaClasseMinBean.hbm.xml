<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="it.bod.application.beans.DocfaClasseMinBean" table="DOCFA_CLASSI_MIN">
  
	<composite-id mapped="false" unsaved-value="undefined">
	   <key-property column="ZC" name="zc" type="java.lang.String"/>
	   <key-property column="CATEGORIA" name="categoria" type="java.lang.String"/>
	   <key-property column="FOGLIO" name="foglio" type="java.lang.String"/>
    </composite-id>

  <property column="OLD_MICROZONA" generated="never" lazy="false" name="old_microzona" type="java.lang.String"/>
  <property column="NEW_MICROZONA" generated="never" lazy="false" name="new_microzona" type="java.lang.String"/>
  <property column="CLASSE_MIN" generated="never" lazy="false" name="classe_min" type="java.lang.String"/>
  
 </class>
</hibernate-mapping>
