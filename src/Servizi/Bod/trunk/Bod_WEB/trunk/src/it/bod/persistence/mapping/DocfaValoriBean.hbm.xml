<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="it.bod.application.beans.DocfaValoriBean" table="DOCFA_VALORI">
  
  <composite-id mapped="false" unsaved-value="undefined">
	   <key-property column="ZONA" name="zona" type="java.lang.String"/>
	   <key-property column="MICROZONA" name="microzona" type="java.lang.String"/>
	   <key-property column="TIPOLOGIA_EDILIZIA" name="tipologiaEdilizia" type="java.lang.String"/>
	   <key-property column="STATO" name="stato" type="java.lang.String"/>
    </composite-id>
    
 
   <property column="VAL_MED" generated="never" lazy="false"
   name="valMed" type="java.math.BigDecimal"/>
  
 </class>
</hibernate-mapping>
