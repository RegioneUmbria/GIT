 <navigation-rule>
  <display-name>Menu inserisce richiesta</display-name>
  <from-view-id>*</from-view-id>
  <navigation-case>
   <from-outcome>menu.richiesta.inserisci</from-outcome>
   <to-view-id>/jsp/protected/richieste/richieste.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>menu.richiesta.filtro</from-outcome>
   <to-view-id>/jsp/protected/filtro/filtroRichieste.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtro.richiesta.filtro</from-outcome>
   <to-view-id>/jsp/protected/filtro/filtroRichieste.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtro.richiesta.fasEvaso</from-outcome>
   <to-view-id>/jsp/protected/filtro/fascicoloEvaso.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>filtro.richiesta.fasNonEvaso</from-outcome>
   <to-view-id>/jsp/protected/filtro/fascicoloNonEvaso.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>fab.dettaglio</from-outcome>
   <to-view-id>/jsp/protected/dettaglio/dettaglioFasFab.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>fab.filtro.dettaglio.back</from-outcome>
   <to-view-id>/jsp/protected/filtro/filtroRichieste.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>ff.logout</from-outcome>
   <to-view-id>/jsp/logout.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
